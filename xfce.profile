[grid]
s0_outline_color = #00042a9f
s0_fill_color = #0008564f

[winrules]
s0_skiptaskbar_match = class=Conky
s0_skippager_match = class=Conky
s0_sticky_match = class=Conky
s0_no_move_match = class=Conky
s0_no_resize_match = class=Conky
s0_no_minimize_match = class=Conky
s0_no_maximize_match = class=Conky
s0_no_close_match = class=Conky
s0_no_focus_match = class=Conky

[expo]
s0_expo_edge = TopLeft
s0_x_offset = 2
s0_y_offset = 2
s0_distance = 0.000000
s0_selected_color = #00042dff

[resize]
s0_border_color = #00042a9f
s0_fill_color = #0008564f

[decor]
s0_shadow_radius = 0.100000
s0_shadow_opacity = 0.010000
s0_shadow_x_offset = -10
s0_shadow_y_offset = 0

[scale]
s0_initiate_edge = BottomRight
s0_initiate_key = Disabled
s0_initiate_all_edge = TopRight

[core]
s0_active_plugins = core;bailer;detection;composite;opengl;compiztoolbox;decor;gnomecompat;vpswitch;staticswitcher;place;grid;regex;winrules;move;session;imgpng;mousepoll;resize;snap;wall;animation;fade;expo;workarounds;scale;
s0_hsize = 3
s0_vsize = 3

[gnomecompat]
s0_command_terminal = xfce4-terminal

[staticswitcher]
s0_popup_delay = 0.000000

[place]
s0_position_matches = (role=buddy_list) & class=Pidgin;
s0_position_x_values = 900;
s0_position_y_values = 20;
s0_position_constrain_workarea = true;
s0_viewport_matches = class=Pidgin;class=Skype;class=Eclipse;class=Google-chrome;class=RSSOwl;class=Vlc;class=Vmware;class=VirtualBox;
s0_viewport_x_values = 1;1;3;2;3;3;2;2;
s0_viewport_y_values = 3;3;1;2;3;2;3;3;

